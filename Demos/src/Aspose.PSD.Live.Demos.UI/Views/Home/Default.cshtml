
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>@Model.Resources["psdPageMainTitle"] </h1>
				<h4>@Model.Resources["psdPageSubHeading"] </h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_psd-brand.png" alt="Aspose.Eps" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposePSD"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.PSD for .NET" />
					</div>
					<a href="/psd/conversion"><span class="spanclass">@Model.Resources["AsposePSD"] @Model.Resources["AsposeProductFor"]  <em>Conversion</em></span><em>Free App offering features to easily convert Photoshop files to raster image formats.</em></a>
				</div>
			</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_viewer-app.png" alt="Aspose.PSD Product Family" />
					</div>
					<a href="/psd/viewer"><span class="spanclass">@Model.Resources["AsposePSD"] @Model.Resources["AsposeProductFor"]  <em>@Model.Resources["Viewerh4"] </em></span><em>View Photoshop files in image format, online from anywhere, any platform.</em></a>
				</div>
				
			</div>

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>